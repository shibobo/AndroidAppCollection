<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shibobo.coolweather">

    <application android:allowBackup="true" android:icon="@drawable/icon"
        android:label="@string/app_name" android:supportsRtl="true" android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".activity.ChooseAreaActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.WeatherActivity"></activity>
        <activity android:name=".activity.TestActivity"></activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

</manifest>